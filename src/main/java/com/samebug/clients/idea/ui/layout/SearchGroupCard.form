<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.samebug.clients.idea.ui.layout.SearchGroupCard">
  <grid id="d06b5" binding="controlPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="478" height="435"/>
    </constraints>
    <properties>
      <maximumSize width="2147483647" height="150"/>
      <preferredSize width="400" height="150"/>
    </properties>
    <border type="empty"/>
    <children>
      <grid id="27dc6" binding="paddingPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="empty">
          <size top="0" left="10" bottom="5" right="10"/>
        </border>
        <children>
          <grid id="6d3d0" binding="infoBar" layout-manager="GridBagLayout">
            <constraints border-constraint="North"/>
            <properties/>
            <border type="empty">
              <size top="3" left="0" bottom="0" right="0"/>
            </border>
            <children>
              <component id="fd8d0" class="javax.swing.JLabel" binding="hitsLabel">
                <constraints>
                  <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="4" fill="0" indent="0" use-parent-layout="false"/>
                  <gridbag weightx="0.3" weighty="0.0"/>
                </constraints>
                <properties>
                  <horizontalAlignment value="4"/>
                  <horizontalTextPosition value="4"/>
                  <text value="100+ hits"/>
                </properties>
              </component>
              <component id="c3bfc" class="javax.swing.JLabel" binding="firstTimeLabel">
                <constraints>
                  <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                  <gridbag weightx="0.3" weighty="0.0"/>
                </constraints>
                <properties>
                  <horizontalAlignment value="2"/>
                  <horizontalTextPosition value="2"/>
                  <text value="moments ago | 3 times, first 2 days ago"/>
                </properties>
              </component>
            </children>
          </grid>
          <grid id="8f3f0" binding="breadcrumbPanel" layout-manager="FlowLayout" hgap="0" vgap="0" flow-align="0">
            <constraints border-constraint="South"/>
            <properties/>
            <border type="empty">
              <size top="0" left="0" bottom="0" right="0"/>
            </border>
            <children>
              <component id="7752a" class="javax.swing.JLabel" binding="breadcrumbBar">
                <constraints/>
                <properties>
                  <opaque value="false"/>
                  <text value="\ IDEA &lt; Android Studio &lt; Android Platform&#10;"/>
                </properties>
              </component>
            </children>
          </grid>
          <grid id="fb99c" binding="contentPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
            <constraints border-constraint="Center"/>
            <properties/>
            <border type="empty">
              <size top="0" left="0" bottom="0" right="0"/>
            </border>
            <children>
              <component id="881e9" class="javax.swing.JLabel" binding="messageLabel">
                <constraints border-constraint="Center"/>
                <properties>
                  <enabled value="true"/>
                  <font swing-font="TextArea.font"/>
                  <text value="asdf"/>
                  <verticalAlignment value="1"/>
                </properties>
              </component>
              <grid id="66fd2" layout-manager="GridBagLayout">
                <constraints border-constraint="North"/>
                <properties/>
                <border type="none"/>
                <children/>
              </grid>
            </children>
          </grid>
        </children>
      </grid>
    </children>
  </grid>
</form>
